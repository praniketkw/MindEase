# Production Environment Configuration for MindEase Frontend

# API Configuration
REACT_APP_API_BASE_URL=${REACT_APP_API_BASE_URL}
REACT_APP_ENVIRONMENT=production

# Application Configuration
REACT_APP_NAME=MindEase
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=Empathetic AI Mental Health Support

# Feature Flags
REACT_APP_VOICE_ENABLED=true
REACT_APP_MULTILINGUAL_ENABLED=false
REACT_APP_ANALYTICS_ENABLED=false

# Crisis Resources
REACT_APP_CRISIS_HOTLINE=988
REACT_APP_CRISIS_TEXT=Text HOME to 741741
REACT_APP_UNIVERSITY_COUNSELING=Contact your university counseling center

# Security Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false